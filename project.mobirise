              },
              "&:hover": {
                "> div": {
                  "transform": "scale(0.95)"
                }
              }
            },
            ".carousel-control-prev": {
              ".mbri-left:before": {
                "content": "\"\\e90a\""
              }
            },
            ".carousel-control-next": {
              ".mbri-right:before": {
                "content": "\"\\e909\""
              }
            },
            ".modal-body a.close": {
              "border-radius": "0",
              "font-size": "32px",
              "top": "0",
              "right": "0",
              "height": "70px",
              "width": "70px",
              "line-height": "70px"
            },
            "@media (max-width: 991px)": {
              ".mbr-section-subtitle": {
                "margin-bottom": "20px",
                "text-align": "center"
              },
              ".mbr-section-title": {
                "text-align": "center"
              },
              ".image": {
                "top": "auto",
                "height": "420px"
              },
              ".mbr-gallery-filter": {
                ".btn": {
                  "padding": "0 0 22px"
                },
                "ul li": {
                  "margin-bottom": "10px"
                }
              },
              ".mbr-gallery-item": {
                "> div img, div:before": {
                  "height": "auto",
                  "border-radius": "3px"
                },
                "> div": {
                  "border-radius": "3px"
                }
              }
            },
            "@media (max-width: 767px)": {
              ".image": {
                "height": "290px"
              }
            }
